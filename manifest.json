{
  "name": "TJDict",
  "short_name": "TJDict",
  "description": "英漢、漢英、英英、日日網路整合字典",
  "version": "1.10.0",
  "manifest_version": 2,
  "permissions": ["<all_urls>", "tabs", "storage", "notifications", "contextMenus", "tts"],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon16.png",
      "38": "img/icon48.png"
    },
    "default_title": "TJDict"
  },
  "options_page": "index.html#options",
  "background": {
    "scripts": ["changelog.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}